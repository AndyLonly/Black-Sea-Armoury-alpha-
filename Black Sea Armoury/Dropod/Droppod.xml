<?xml version="1.0" encoding="utf-8"?>
<Items>
<!-- Drop pod -->
  <Item name="Pirategunner_Pod" identifier="BW_P_gunner" nameidentifier="BW_P_gunner" translationidentifier="BW_P_gunner" category="Misc" scale="0.4" sonarsize="6" hideinmenus="true">
    <Sprite texture="%ModDir%/Dropod/ES_RUSH.png" sourcerect="12,72,897,843" depth="0.55" />
    <Body width="600" height="110" density="20" />
    <Projectile characterusable="false" launchimpulse="100" maxtargetstohit="3" sticktocharacters="false" sticktoitems="false" sticktostructures="false" friendlyfire="false" damagedoors="true">
      <Attack structuredamage="100" itemdamage="10" severlimbsprobability="2.0" penetration="0.3">
        <Affliction identifier="lacerations" strength="1" />
        <Affliction identifier="bleeding" strength="1" />
        <Affliction identifier="stun" strength="1" />
      </Attack>
      <StatusEffect type="OnActive" target="This" lifetime="0.5">
        <ParticleEmitter particle="ammotrailwater" copyentityangle="true" anglemin="0" anglemax="0" particleamount="10" velocitymin="-10" velocitymax="-100" scalemin="0.5" scalemax="1" />
      </StatusEffect>
      <StatusEffect type="OnActive" target="This" stackable="false" delay="6">
        <SpawnCharacter speciesname="Pirategunner" count="1" spread="1" />
        <SpawnCharacter speciesname="Piratescavenger" count="1" spread="1" />
        <Remove />
      </StatusEffect>
      <StatusEffect type="OnImpact" target="UseTarget">
        <Conditional entitytype="eq Structure" />
        <ParticleEmitter particle="shrapnel" copyentityangle="true" anglemin="0" anglemax="360" particleamount="5" velocitymin="100" velocitymax="2000" scalemin="0.1" scalemax="0.2" />
        <!--Add some impact effect-->
        <Explosion ignorecover="true" onlyinside="true" range="600" force="20" playtinnitus="false" showeffects="false" applyfireeffects="false">
          <Affliction identifier="stun" strength="2.5" />
        </Explosion>
      </StatusEffect>
      <StatusEffect type="OnImpact" target="UseTarget">
        <Conditional entitytype="eq Structure" />
        <Conditional hastag="eq door" />
        <ParticleEmitter particle="spark" copyentityangle="true" anglemin="-10" anglemax="10" particleamount="5" velocitymin="-10" velocitymax="-200" scalemin="0.5" scalemax="1" />
      </StatusEffect>
      <StatusEffect type="OnBroken" target="This">
        <Remove />
      </StatusEffect>
    </Projectile>
  </Item>
